<!--pages/user/user.wxml-->

<view class="userinfo">

  
    <block>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
<view class="weui-cells weui-cells_after-title">
            <navigator url="../mysign/mysign?openid={{userInfo.openid}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                    <image src="../../pic/a.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的报名</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            <navigator url="../mytuijian/mytuijian" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                    <image src="../../pic/b.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的推荐</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
           
            <navigator url="../userinfo/userinfo" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                    <image src="../../pic/c.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">修改信息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            <navigator url="../tel/tel" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                    <image src="../../pic/d.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">我的客服</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            <view bindtap='gomap'   class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                    <image src="../../pic/e.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">门店地址</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </view>
            
             
        </view>
